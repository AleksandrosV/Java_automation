<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="FullUITestSuite" verbose="1" parallel="tests" thread-count="4">
	<test name="PositiveLogInTest-CH">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class
				name="com.herokuapp.theinternet.loginpagetests.PositiveLogInTests" />
		</classes>
	</test>

	<test name="NegativeUsernameTest-CH">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class
				name="com.herokuapp.theinternet.loginpagetests.NegativeLogInTests">
			</class>
		</classes>
	</test>

	<test name="NegativePasswordTest-FF">
		<parameter name="browser" value="firefox"></parameter>
		<classes>
			<class
				name="com.herokuapp.theinternet.loginpagetests.NegativeLogInTests">
			</class>
		</classes>
	</test>

	<test name="CheckboxesTests">
		<classes>
			<class
				name="com.herokuapp.theinternet.checkboxespagetests.CheckboxesTests" />
		</classes>
	</test>
</suite>